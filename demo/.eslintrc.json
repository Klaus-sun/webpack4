{
    "parserOptions": {
        "ecmaVersion": 5,
        "sourceType": "script",
        "ecmaFeatures": {}
    },
    "rules": {
        "no-div-regex": 2,
        "no-delete-var": 2,
        "no-eq-null": 2,
        "no-useless-call": 2,
        "no-useless-catch": 2,
        "no-label-var": 2
    },
    "env": {
        "node": true
    }
}